{
    "#":"# 相对目录以src目录为基准",

    "lib": {
        "vue$": "vue/dist/vue.esm.js"
    },
    "favicon":"./assets/images/fish3.png",
    "pathHtml":"./entries/*.html",
    "pathJs":"./entries/*.js",
    "pathTs":"./entries/*.ts",
    "pathStatic":"./static",
    "pathServiceWorkers":"./serviceworkers/index.js",
    "WebpackSinglePlugin":[
        {
            "template":"index.html",
            "source":"../node_modules/vue/dist/vue.min.js",
            "target":"static/vue.min.js"
        }
    ],
    "WebpackPWAManifest":{
        "template":"index.html",
        "source":"./serviceworkers/manifest.json",
        "target":"manifest.json",
        "iconTarget":"images/icons/"
    }
}